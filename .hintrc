{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "button-type": "off",
    "meta-viewport": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "highest-available-document-mode": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scale"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  }
}